{
  "name": "@typexs/ng",
  "version": "1.5.0",
  "description": "Angular for typexs",
  "main": "index.js",
  "browser": "browser.js",
  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "npx gulp test",
    "posttest": "npx nyc report --reporter=json && npx codecov -f coverage/**/*.json",
    "ng": "npx ng",
    "start": "npx concurrently \"npx ng serve\" \"npx typexs server --nodeId server\"",
    "build": "npx ng build --prod",
    "lint": "npx ng lint",
    "mocha": "npx mocha test/{**,**/**}/*.spec.ts",
    "karma": "npx ng test typexs-ng --code-coverage=true --watch=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typexs/typexs-ng.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "angular",
    "framework",
    "commons",
    "typescript",
    "typexs"
  ],
  "bugs": {
    "url": "https://github.com/typexs/typexs-ng/issues"
  },
  "homepage": "https://github.com/typexs/typexs-ng#readme",
  "devDependencies": {
    "@allgemein/moduls": "^1.6.5",
    "@allgemein/packaging": "^1.2.0",
    "@allgemein/schema-api": "^1.4.4",
    "@angular-devkit/build-angular": "12.0.3",
    "@angular-devkit/core": "12.0.3",
    "@angular-devkit/schematics": "12.0.3",
    "@angular-eslint/eslint-plugin": "^12.1.0",
    "@angular/animations": "12.0.3",
    "@angular/cli": "12.0.3",
    "@angular/common": "12.0.3",
    "@angular/compiler": "12.0.3",
    "@angular/compiler-cli": "12.0.3",
    "@angular/core": "12.0.3",
    "@angular/forms": "12.0.3",
    "@angular/language-service": "12.0.3",
    "@angular/platform-browser": "12.0.3",
    "@angular/platform-browser-dynamic": "12.0.3",
    "@angular/router": "12.0.3",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@popperjs/core": "^2.9.2",
    "@schematics/angular": "12.0.3",
    "@testdeck/mocha": "^0.1.2",
    "@types/chai": "^4.2.18",
    "@types/d3": "^6.7.0",
    "@types/express": "^4.17.12",
    "@types/glob": "^7.1.3",
    "@types/jasmine": "^3.7.6",
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.0",
    "@types/supertest": "^2.0.11",
    "@types/through2": "^2.0.36",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@typexs/base": "^1.5.9",
    "@typexs/base-ng": "file:./packages/base-ng",
    "@typexs/ng": "file:./packages/ng",
    "@typexs/ng-router-menu": "file:./packages/ng-router-menu",
    "@typexs/schema": "^1.5.4",
    "@typexs/server": "^1.5.3",
    "ag-grid-angular": "^25.3.0",
    "ag-grid-community": "^25.3.0",
    "ajv": "^8.5.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.0.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.2",
    "core-js": "^3.13.1",
    "d3": "^6.7.0",
    "del": "^6.0.0",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "express": "^4.17.1",
    "glob": "^7.1.7",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-mocha": "^8.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-shell": "^0.8.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^5.0.1",
    "jasmine": "^3.7.0",
    "jquery": "^3.6.0",
    "js-yaml": "^4.1.0",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "light-server": "^2.9.1",
    "lodash": "^4.17.21",
    "merge-stream": "^2.0.0",
    "mocha": "^8.4.0",
    "ng-packagr": "12.0.3",
    "nyc": "^15.1.0",
    "perfect-scrollbar": "^1.5.1",
    "prismjs": "^1.23.0",
    "redis": "^3.1.2",
    "rxjs": "6.6.7",
    "rxjs-compat": "6.6.7",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "source-map": "^0.7.3",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "through": "^2.3.8",
    "ts-node": "^10.0.0",
    "typeorm": "0.2.34",
    "typescript": "4.2.4",
    "winston": "^3.3.3",
    "yargs": "^17.0.1",
    "zone.js": "~0.11.4"
  },
  "dependencies": {
    "@typexs/roles-api": "~1.5.0"
  },
  "peerDependencies": {
    "@typexs/base": "~1.5.9",
    "@typexs/schema": "~1.5.4",
    "@typexs/server": "~1.5.3"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "json",
      "lcovonly"
    ],
    "report-dir": "coverage/backend",
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "typexs-ng",
    "declareLibs": [
      {
        "topic": "server.controllers",
        "refs": [
          "modules/*/api",
          "src/modules/*/api"
        ]
      },
      {
        "topic": "entity.default",
        "refs": [
          "modules/*/entities",
          "src/modules/*/entities"
        ]
      }
    ]
  }
}
