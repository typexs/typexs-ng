{
  "name": "typexs-ng",
  "version": "0.3.11",
  "description": "Angular for typexs",
  "main": "index.js",
  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "nyc mocha --opts test/mocha.all.opts",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "ng": "ng",
    "start": "concurrently \"ng serve -pc proxy.conf.json --open\" \"typexs server\"",
    "build": "ng build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng-packagr -p bundles/package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typexs/typexs-ng.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/typexs/typexs-ng/issues"
  },
  "homepage": "https://github.com/typexs/typexs-ng#readme",
  "devDependencies": {
    "@angular-devkit/core": "^0.3.2",
    "@angular-devkit/schematics": "^0.3.2",
    "@angular/cli": "^1.7.4",
    "@angular/compiler": "^5.2.10",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/language-service": "^5.2.10",
    "@types/chai": "^4.1.3",
    "@types/deepmerge": "^2.1.0",
    "@types/express": "^4.11.1",
    "@types/glob": "^5.0.35",
    "@types/gulp": "^4.0.5",
    "@types/gulp-watch": "^4.1.34",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.8",
    "@types/semver": "^5.5.0",
    "@types/source-map": "^0.5.7",
    "@types/string-template": "^1.0.2",
    "@types/superagent": "^3.5.8",
    "@types/supertest": "^2.0.4",
    "@types/supertest-as-promised": "^2.0.35",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.1",
    "codelyzer": "^4.3.0",
    "del": "^3.0.0",
    "express": "^4.16.3",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-clean": "^0.4.0",
    "gulp-ngc": "^0.3.3",
    "gulp-replace": "^0.6.1",
    "gulp-shell": "^0.6.5",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.0",
    "gulpclass": "^0.1.2",
    "karma": "~2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^5.1.1",
    "mocha-typescript": "^1.1.12",
    "ng-packagr": "^3.0.6",
    "ngc-webpack": "^4.1.2",
    "nyc": "^11.7.1",
    "protractor": "~5.3.1",
    "rollup": "^0.58.2",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "sqlite3": "^4.0.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2",
    "ts-node": "^6.0.2",
    "tslint": "~5.9.1",
    "typescript": "^2.8.3",
    "typescript-require": "^0.2.10",
    "uglify-js": "^3.3.23",
    "webpack-stream": "^4.0.3"
  },
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@ng-bootstrap/ng-bootstrap": "^1.1.2",
    "@schematics/angular": "^0.3.2",
    "ajv": "^6.4.0",
    "bootstrap": "^4.1.0",
    "case": "^1.5.4",
    "concurrently": "^3.6.1",
    "core-js": "^2.5.5",
    "gulp-debug": "^3.2.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.10",
    "semver": "^5.5.0",
    "string-template": "^1.0.0",
    "typexs-base": "latest",
    "typexs-schema": "latest",
    "typexs-server": "latest",
    "zone.js": "^0.8.26"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "typexs-ng",
    "declareLibs": [
      {
        "topic": "server.controllers",
        "refs": [
          "modules/*/api",
          "src/modules/*/api"
        ]
      },
      {
        "topic": "entity.default",
        "refs": [
          "modules/*/entities",
          "src/modules/*/entities"
        ]
      }
    ]
  }
}
