{
  "name": "@typexs/ng",
  "version": "0.4.8",
  "description": "Angular for typexs",
  "main": "index.js",
  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "nyc mocha --opts test/mocha.all.opts && karma start --single-run --browsers ChromeHeadless --code-coverage",
    "posttest": "nyc report --reporter=json && codecov -f coverage/backend/*.json && codecov -f coverage/karma/*.json",
    "ng": "ng",
    "start": "concurrently \"ng serve -pc proxy.conf.json --open\" \"typexs server\"",
    "build": "ng build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mocha": "mocha --opts test/mocha.all.opts",
    "karma": "karma start --single-run --browsers ChromeHeadless",
    "build:lib": "ng-packagr -p bundles/package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typexs/typexs-ng.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/typexs/typexs-ng/issues"
  },
  "homepage": "https://github.com/typexs/typexs-ng#readme",
  "devDependencies": {
    "@angular-devkit/core": "0.3.2",
    "@angular-devkit/schematics": "0.3.2",
    "@angular/cli": "1.7.4",
    "@angular/compiler": "^5.2.10",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/language-service": "^5.2.10",
    "@types/chai": "^4.1.6",
    "@types/deepmerge": "^2.1.0",
    "@types/express": "^4.16.0",
    "@types/glob": "^7.1.1",
    "@types/gulp": "^4.0.5",
    "@types/gulp-watch": "^4.1.34",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.3",
    "@types/semver": "^5.5.0",
    "@types/string-template": "^1.0.2",
    "@types/superagent": "^3.8.4",
    "@types/supertest": "^2.0.6",
    "@types/supertest-as-promised": "^2.0.36",
    "@types/winston": "^2.4.4",
    "bootstrap": "^4.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.1.0",
    "codelyzer": "^4.4.4",
    "core-js": "^2.5.7",
    "del": "^3.0.0",
    "express": "^4.16.3",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.6.5",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.1",
    "gulpclass": "^0.1.2",
    "jquery": "^3.3.1",
    "karma": "~3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-es6-shim": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "ng-packagr": "^3.0.6",
    "nyc": "^13.0.1",
    "popper.js": "^1.14.4",
    "protractor": "~5.4.1",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "source-map": "^0.7.3",
    "sqlite3": "^4.0.2",
    "supertest": "^3.3.0",
    "supertest-as-promised": "^4.0.2",
    "ts-node": "^7.0.1",
    "typescript": "3.1.1",
    "typescript-require": "^0.2.10"
  },
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@schematics/angular": "^0.3.2",
    "ajv": "^6.4.0",
    "rxjs": "^5.5.10",
    "semver": "^5.5.0",
    "string-template": "^1.0.0",
    "@typexs/base": "latest",
    "@typexs/schema": "latest",
    "@typexs/server": "latest",
    "zone.js": "^0.8.26"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "typexs-ng",
    "declareLibs": [
      {
        "topic": "server.controllers",
        "refs": [
          "modules/*/api",
          "src/modules/*/api"
        ]
      },
      {
        "topic": "entity.default",
        "refs": [
          "modules/*/entities",
          "src/modules/*/entities"
        ]
      }
    ]
  }
}
