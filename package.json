{
  "name": "@typexs/ng",
  "version": "1.3.0",
  "description": "Angular for typexs",
  "main": "index.js",
  "browser": "browser.js",
  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "npx nyc mocha  test/{**,**/**}/*.spec.ts && npx ng test typexs-ng --code-coverage=true --watch=false",
    "posttest": "npx nyc report --reporter=json && npx codecov -f coverage/backend/*.json && npx codecov -f coverage/karma/*.json",
    "ng": "npx ng",
    "start": "npx concurrently \"npx ng serve\" \"npx typexs server --nodeId server\"",
    "build": "npx ng build --prod",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e",
    "mocha": "npx mocha --opts test/mocha.all.opts",
    "karma": "npx ng test typexs-ng --code-coverage=true --watch=false",
    "build:lib": "npx ng-packagr -p bundles/package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typexs/typexs-ng.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "angular",
    "framework",
    "commons",
    "typescript",
    "typexs"
  ],
  "bugs": {
    "url": "https://github.com/typexs/typexs-ng/issues"
  },
  "homepage": "https://github.com/typexs/typexs-ng#readme",
  "devDependencies": {
    "@allgemein/packaging": "^1.1.0",
    "@angular-devkit/build-angular": "^0.1101.4",
    "@angular-devkit/core": "11.1.4",
    "@angular-devkit/schematics": "11.1.4",
    "@angular/animations": "^11.1.2",
    "@angular/cli": "^11.1.4",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/compiler-cli": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/forms": "^11.1.2",
    "@angular/language-service": "^11.1.2",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@schematics/angular": "^11.1.4",
    "@testdeck/mocha": "^0.1.2",
    "@types/chai": "^4.2.14",
    "@types/d3": "^6.3.0",
    "@types/express": "^4.17.11",
    "@types/glob": "^7.1.3",
    "@types/gulp": "^4.0.8",
    "@types/gulp-bump": "^2.8.1",
    "@types/gulp-replace": "0.0.31",
    "@types/gulp-shell": "0.7.0",
    "@types/gulp-sourcemaps": "0.0.34",
    "@types/jasmine": "^3.6.3",
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.25",
    "@types/semver": "^7.3.4",
    "@types/string-template": "^1.0.2",
    "@types/superagent": "^4.1.10",
    "@types/supertest": "^2.0.10",
    "@typexs/base": "~1.3.3",
    "@typexs/schema": "~1.1.0",
    "@typexs/server": "~1.1.0",
    "ag-grid-angular": "^25.0.1",
    "ag-grid-community": "^25.0.1",
    "ajv": "^7.0.4",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.6.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.1",
    "codelyzer": "^6.0.1",
    "core-js": "^3.8.3",
    "d3": "^6.5.0",
    "del": "^6.0.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.8.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^5.0.1",
    "jasmine": "^3.6.4",
    "jquery": "^3.5.1",
    "karma": "^6.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "merge-stream": "^2.0.0",
    "mocha": "^8.2.1",
    "ng-packagr": "^11.1.4",
    "nyc": "^15.1.0",
    "perfect-scrollbar": "^1.5.0",
    "popper.js": "^1.16.1",
    "prismjs": "^1.23.0",
    "protractor": "~7.0.0",
    "redis": "^3.0.2",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.6.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "source-map": "^0.7.3",
    "sqlite3": "^5.0.1",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "4.1.3",
    "typeorm": "0.2.28",
    "winston": "^3.3.3",
    "yargs": "^16.2.0",
    "js-yaml": "^4.0.0",
    "zone.js": "~0.11.3"
  },
  "dependencies": {
    "@typexs/roles-api": "~1.0.19"
  },
  "peerDependencies": {
    "@typexs/base": "~1.3.3",
    "@typexs/schema": "~1.1.0",
    "@typexs/server": "~1.1.0"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "json",
      "lcovonly"
    ],
    "report-dir": "coverage/backend",
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "typexs-ng",
    "declareLibs": [
      {
        "topic": "server.controllers",
        "refs": [
          "modules/*/api",
          "src/modules/*/api"
        ]
      },
      {
        "topic": "entity.default",
        "refs": [
          "modules/*/entities",
          "src/modules/*/entities"
        ]
      }
    ]
  }
}
